{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["build/**", "dist/**"]
    },
    "dev": {
      "cache": false,
      "dependsOn": ["^build"],
      "persistent": true
    },
    "test": {
      "dependsOn": ["^build"]
    },
    "report": {
      "dependsOn": ["^test"]
    }
  }
}
